  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:local": "ng build -c local",
    "build:dev": "ng build -c dev",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "fix:prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "eslint": "npx eslint ./src",
    "fix:eslint": "npx eslint ./src --fix",
    "fix": "npm run fix:prettier && npm run fix:eslint"
  },
